# Backup Configuration
COMPRESSION_LEVEL=6
ENCRYPTION_ENABLED=false
ENCRYPTION_KEY=your_encryption_key_here
RETENTION_DAYS=30
MAX_BACKUP_SIZE_GB=100

# Remote Backup Server Configuration
BACKUP_SERVER_IP=192.168.1.100
BACKUP_SERVER_PORT=22
BACKUP_SERVER_USER=backup_user
BACKUP_SERVER_PATH=/backup/storage
BACKUP_SERVER_SSH_KEY=/path/to/ssh/private/key

# Schedule Configuration
FULL_BACKUP_DAY=Sunday
FULL_BACKUP_TIME=02:00
INCREMENTAL_BACKUP_TIMES=10:00,14:00,18:00

# Logging Configuration
LOG_LEVEL=INFO

# Local Paths (optional)
LOCAL_TEMP_DIR=/path/to/temp/directory
LOG_DIR=/path/to/log/directory 